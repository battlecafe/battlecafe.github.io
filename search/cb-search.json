---
layout: null
sitemap:
  exclude: true
---
{
	"code" : 0 ,
	"data" : [
	{% for post in site.posts %}
    {
        "title": "{{ post.title | escape }} - {{ post.subtitle | escape }}",
        "tags": "{{ post.tags | join: ' â€¢ ' }}",
        "date": "{{ post.date }}",
        "content": "{{ post.content | strip_html | strip_newlines | escape |replace:'\','@@' }}",
        "url": "{{ site.baseurl }}{{ post.url }}"
    }
    {% if forloop.rindex != 1  %}
	,
	{% endif %}
    {% endfor %}
	         ]
}