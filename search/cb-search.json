---
layout: null
sitemap:
  exclude: true
---
{
	"code" : 0 ,
	"data" : [
	{% for post in site.posts %}
    {
      "title": "{{ post.title | escape }}",
      "subtitle": "{{ post.subtutle | escape }}",
      "tag": "{{ post.tags | join: ', ' }}",
      "url": "{{ site.baseurl }}{{ post.url }}",
      "date": "{{ post.date }}",
      "content": "{{ post.content | strip_html | strip_newlines | escape |replace:'\','@@' }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
	         ]